<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.erp.salesman">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SalesMan"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".stockList"
            android:exported="false" />
        <activity
            android:name=".Order_Activity"
            android:exported="false" />
        <activity
            android:name=".clients_creation_activity"
            android:exported="false" />
        <activity
            android:name=".Clients_Activity"
            android:exported="false" />
        <activity
            android:name=".SalesActivity"
            android:exported="false" />
        <activity
            android:name=".stoke_Activity"
            android:exported="false" />
        <activity
            android:name=".erp_main_page_Activity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>